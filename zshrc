# fi

# Compilation flags
# export ARCHFLAGS="-arch x86_64"

# Set personal aliases, overriding those provided by oh-my-zsh libs,
# plugins, and themes. Aliases can be placed here, though oh-my-zsh
# users are encouraged to define aliases within the ZSH_CUSTOM folder.
# For a full list of active aliases, run `alias`.
#
# Example aliases
# alias zshconfig="mate ~/.zshrc"
# alias ohmyzsh="mate ~/.oh-my-zsh"

# Refresh source : source ~/.zshrc 

# Auto suggest
export ZSH_AUTOSUGGEST_HIGHLIGHT_STYLE="fg=6"

# aliases

# react - native
# Build apk file
# ./gradlew assembleRelease
# Build abb file
# ./gradlew bundleRelease
# init new project
# react-native init newproject --version react-native@0.61.0

alias rnsr="npx react-native start --reset-cache"
alias rnra="npx react-native run-android"
alias rnri='npx react-native run-ios --simulator="iPhone X"'
alias rnri6='npx react-native run-ios --simulator="iPhone 6"'
alias rnri8='npx react-native run-ios --simulator="iPhone 8"'
alias rnri8Plus='npx react-native run-ios --simulator="iPhone 8 Plus"'
alias rnri11='npx react-native run-ios --simulator="iPhone 11"'
alias rnri11ProMax='npx react-native run-ios --simulator="iPhone 11 Pro Max"'

function rnDebugOn() {
    if [ "$1" != "" ]
    then
        npx react-native run-ios --device="$1"
    else
        npx react-native run-ios --device="Thientv"
    fi
}

function rnReleaseOn() {
    if [ "$1" != "" ]
    then
        npx react-native run-ios --configuration Release --device="$1"
    else
        npx react-native run-ios --configuration Release --device="Thientv"
    fi
}


# Add rbenv to bash so that it loads every time you open a terminal
echo 'if which rbenv > /dev/null; then eval "$(rbenv init -)"; fi' >> ~/.bash_profile

# fastlane
export PATH="$HOME/.fastlane/bin:$PATH"
export LC_ALL=en_US.UTF-8
export LANG=en_US.UTF-8

# android
export PATH=$PATH:/Users/Thientv/Library/Android/sdk/platform-tools/
export ANDROID_HOME=$HOME/Library/Android/sdk
export PATH=$PATH:$ANDROID_HOME/emulator
export PATH=$PATH:$ANDROID_HOME/tools
export PATH=$PATH:$ANDROID_HOME/tools/bin
export PATH=$PATH:$ANDROID_HOME/platform-tools

# open chrome
alias "openChrome"='open -a "Google Chrome"'
# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/thientv/Downloads/google-cloud-sdk/path.zsh.inc' ]; then . '/Users/thientv/Downloads/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/thientv/Downloads/google-cloud-sdk/completion.zsh.inc' ]; then . '/Users/thientv/Downloads/google-cloud-sdk/completion.zsh.inc'; fi

#GOOGLE_APPLICATION_CREDENTIALS
#export GOOGLE_APPLICATION_CREDENTIALS="~/Users/thientv/Mimas/Project/minshoes-firebase/admin.json"

# Flutter
export PATH="$PATH:/Users/thientv/flutter/bin"
if which rbenv > /dev/null; then eval "$(rbenv init -)"; fi

